{
  "manifest_version": 2,
  "name": "My Separator",
  "version": "0.1.0",
  "homepage_url": "https://github.com/LightAPIs/my-separator",
  "description": "My Chrome Extension",
  "author": "pana",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "My Separator"
  },
  "permissions": ["bookmarks"]
}
